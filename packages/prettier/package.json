{
  "name": "@yopem/prettier-config",
  "version": "0.1.10",
  "description": "",
  "keywords": [
    "config",
    "prettier",
    "prettier-config"
  ],
  "bugs": {
    "url": "https://github.com/yopem/config/issues"
  },
  "license": "MIT",
  "author": "Karyana Yandi <karyana@yandi.me>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yopem/config.git",
    "directory": "packages/config/prettier"
  },
  "files": [
    "src"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "src/base.js",
  "module": "src/base.js",
  "types": "src/base.js",
  "exports": {
    "./base": {
      "source": "./src/base.js",
      "import": {
        "types": "./src/base.js",
        "default": "./src/base.js"
      }
    },
    "./react": {
      "source": "./src/react.js",
      "import": {
        "types": "./src/react.js",
        "default": "./src/react.js"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1"
  },
  "devDependencies": {
    "@yopem/typescript-config": "workspace:*",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11 || ^0.7.0"
  }
}
