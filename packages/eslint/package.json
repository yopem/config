{
  "name": "@yopem/eslint-config",
  "version": "0.3.1",
  "description": "",
  "keywords": [
    "eslint",
    "eslint-config",
    "config"
  ],
  "type": "module",
  "main": "src/base.js",
  "module": "src/base.js",
  "types": "src/types.d.ts",
  "files": [
    "src"
  ],
  "exports": {
    "./base": {
      "source": "./src/base.js",
      "import": {
        "types": "./src/types.d.ts",
        "default": "./src/base.js"
      }
    },
    "./nextjs": {
      "source": "./src/nextjs.js",
      "import": {
        "types": "./src/types.d.ts",
        "default": "./src/nextjs.js"
      }
    },
    "./react": {
      "source": "./src/react.js",
      "import": {
        "types": "./src/types.d.ts",
        "default": "./src/react.js"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "Karyana Yandi <karyana@yandi.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yopem/config",
    "directory": "packages/config/eslint"
  },
  "bugs": {
    "url": "https://github.com/yopem/config/issues"
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/compat": "1.4.0",
    "@eslint/js": "9.32.0",
    "@next/eslint-plugin-next": "16.0.1",
    "@types/eslint": "9.6.1",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "typescript-eslint": "8.46.2"
  },
  "devDependencies": {
    "@yopem/typescript-config": "workspace:*",
    "eslint-plugin-turbo": "2.6.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.21.0",
    "@next/eslint-plugin-next": "^15.2.0 || ^16.0.0",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "typescript-eslint": "^8.25.0"
  }
}
